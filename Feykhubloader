-- // Feyk Loader â€“ Supports Hypershot & Bluelock Rivals
local GameId = game.GameId

-- Supported games by GameId
local SupportedGames = {
    [5995470825] = "https://raw.githubusercontent.com/FeykHere/FeykhubHypershot/refs/heads/main/Feykhubhypershot", -- Hypershot
    [18668065416] = "https://raw.githubusercontent.com/FeykHere/FeykhubBlr/refs/heads/main/FeykhubBLR" -- BLR lobby (if needed by PlaceId)
}

-- Notification helper
local function Notify(title, text, duration)
    pcall(function()
        game.StarterGui:SetCore("SendNotification", {
            Title = title,
            Text = text,
            Duration = duration or 5
        })
    end)
end

-- Script loader
local function LoadScript(url)
    local success, result = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not success then
        warn("Script failed to load:", result)
        Notify("Feyk Loader", "Error loading script.", 5)
    end
end

-- Main loader
if SupportedGames[GameId] then
    Notify("Feyk Loader", "Supported Game Detected! Loading...", 4)
    task.wait(2)
    LoadScript(SupportedGames[GameId])
else
    Notify("Feyk Loader", "Game is NOT Supported!\nCheck our Discord for supported games.", 6)
end
