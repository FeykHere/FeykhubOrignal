--// Feyk Loader â€“ Supports Hypershot & Bluelock Rivals

local PlaceId = game.PlaceId
print("Detected PlaceId:", PlaceId)

-- Hypershot
local HypershotPlaceIds = {
    [17516596118] = true, 
    [10842855952] = true, 
    [108428559529058] = true,
    [100040622766961] = true
}

-- Bluelock Rivals
local BluelockPlaceIds = {
    [18668065416] = true 
}

-- Forsaken 
local ForsakenPlaceIds = {
    [18687417158] = true
}

-- Script selector
local ScriptURL = nil

if HypershotPlaceIds[PlaceId] then
    ScriptURL = "https://raw.githubusercontent.com/FeykHere/FeykHubMain/refs/heads/main/FeykhubHyperShotLua.lua"
elseif BluelockPlaceIds[PlaceId] then
    ScriptURL = "https://raw.githubusercontent.com/FeykHere/FeykHubMain/refs/heads/main/FeykhubBLRlua.lua"
elseif ForsakenPlaceIds[PlaceId] then
ScriptURL = "https://raw.githubusercontent.com/FeykHere/FeykHubMain/refs/heads/main/FeykhubForsakenLua.lua"
end

-- Notification helper
local function Notify(title, text, duration)
    pcall(function()
        game.StarterGui:SetCore("SendNotification", {
            Title = title,
            Text = text,
            Duration = duration or 5
        })
    end)
end

-- Script loader
local function LoadScript(url)
    local success, result = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not success then
        warn("Script failed to load:", result)
        Notify("Feyk Loader", "Error loading script:\n" .. tostring(result), 6)
    end
end

-- Loader logic
if ScriptURL then
    Notify("Feyk Loader", "Supported Game Detected! Loading...", 4)
    task.wait(2)
    LoadScript(ScriptURL)
else
    Notify("Feyk Loader", "Game is NOT Supported!\nCheck our Discord for supported games.", 6)
end
